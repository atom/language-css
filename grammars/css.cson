        'include': '#property-values'
      }
      {
        'include': '#string-double'
      }
      {
        'include': '#string-single'
      }
    ]
  }
  {
    'begin': '(?=\\{)'
    'end': '(?!\\G)'
    'patterns': [
      {
        'include': '#rule-list'
      }
    ]
  }
